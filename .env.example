# YouTube RAG Chatbot Environment Variables
# Copy this file to .env and fill in your actual values

# Google API Key (Required)
# Get from: https://console.developers.google.com/
GOOGLE_API_KEY=your_google_api_key_here

# Proxy Configuration (Optional but recommended for better reliability)
# Get from: https://www.webshare.io/ or similar proxy service
PROXY_USERNAME=your_proxy_username
PROXY_PASSWORD=your_proxy_password

# Development Settings
NODE_ENV=development

# For Frontend (Next.js) - Update with your deployed backend URL
NEXT_PUBLIC_API_URL=http://localhost:8000
